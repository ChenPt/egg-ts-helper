{
  "files.exclude": {
    "USE_GITIGNORE": true,
    "**/*.js": {
      "when": "$(basename).ts"
    },
    "**/*.map": true,
    "run": true,
    "logs": true,
    "out": true
  },
  "typescript.tsdk": "node_modules/typescript/lib",
  "prettier.trailingComma": "all",
  "debug.node.autoAttach": "on"
}